<body>

<div id="document" role="document">
  <hgroup id="heading">
    <h1 id="site-title"><span>Abonomatic</span></h1>
    <p id="share"><a href="#">Comparte este mapa</a></p>
  </hgroup>

  <div id="main" role="main">
    <aside>
      <%= render :partial => 'layouts/search' %>

      <div id="plots">
      <% 20.times do %>    
        <%= render :partial => 'layouts/plot' %>
      <% end %> 
      </div>

      <div id="download">
        <p><a href="#">Descargar HSF</a></p>
      </div>
    </aside>

    <%= yield %>
  </div>

  <footer id="contentinfo" role="contentinfo">
    <p>Hecho con <a href="#">CartoDB</a> y croisants</p>
  </footer>
</div>

